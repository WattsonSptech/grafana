          },
          "format": 1,
          "rawSQL": "WITH t AS (\r\n  SELECT AVG(tensao_valor) AS tensao, zona_geracao FROM tensao_clima GROUP BY data_hora_geracao, zona_geracao\r\n)\r\nSELECT \r\n  MAX(t.tensao), coord.zona_geracao, coord.lat, coord.long,\r\n  CASE\r\n    WHEN MAX(t.tensao) >= $max_tensao OR MAX(t.tensao) <= $min_tensao THEN 'rgb(0, 0, 0)'\r\n    ELSE 'rgb(0, 0, 0)'\r\n  END AS cor,\r\n  CASE\r\n    WHEN MAX(t.tensao) >= $max_tensao or MAX(t.tensao) <= $min_tensao THEN CONCAT(coord.zona_geracao, ' - Falha!')\r\n    ELSE CONCAT(coord.zona_geracao, ' - OK')\r\n  END AS legenda\r\nFROM t JOIN (\r\n  VALUES\r\n    ('NORTE', -23.5098368,-46.6674257),\r\n    ('NORDESTE', -23.4902519,-46.5945756),\r\n    ('LESTE', -23.5579273,-46.5824666),\r\n    ('SUDESTE', -23.5909535,-46.6267781),\r\n    ('SUL', -23.6180062,-46.646582),\r\n    ('OESTE', -23.5544304,-46.739646),\r\n    ('CENTRO', -23.5438807, -46.6310787)\r\n) AS coord (zona_geracao, lat, long) ON t.zona_geracao = coord.zona_geracao\r\nGROUP BY coord.zona_geracao, coord.lat, coord.long\r\n-- WHERE t.tensao >= $max_tensao or t.tensao <= $min_tensao",
          "refId": "A"
        },
        {
          "connectionArgs": {
            "catalog": "__default",
            "database": "__default",
            "region": "__default",
            "resultReuseEnabled": false,
            "resultReuseMaxAgeInMinutes": 60
          },
          "datasource": {
            "type": "grafana-athena-datasource",
            "uid": "ff320tqdhx5oga"
          },
          "format": 1,
          "hide": false,
          "rawSQL": "SELECT \r\nDISTINCT coord.zona_geracao, rlat, rlong, clima_vento\r\nFROM tensao_clima as t JOIN (\r\n  VALUES\r\n    ('NORTE', -23.5098368,-46.6674257),\r\n    ('NORDESTE', -23.4902519,-46.5945756),\r\n    ('LESTE', -23.5579273,-46.5824666),\r\n    ('SUDESTE', -23.5909535,-46.6267781),\r\n    ('SUL', -23.6180062,-46.646582),\r\n    ('OESTE', -23.5544304,-46.739646),\r\n    ('CENTRO', -23.5438807, -46.6310787)\r\n) AS coord (zona_geracao, rlat, rlong) ON t.zona_geracao = coord.zona_geracao WHERE clima_vento > 9;",
          "refId": "B"
        }
